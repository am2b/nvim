{
    "languageserver": {
        "swift": {
            "command": "xcrun",
            "args": [
                "--toolchain",
                "swift",
                "sourcekit-lsp"
            ],
            "filetypes": [
                "swift"
            ],
            "rootPatterns": [
                "Package.swift",
                ".git/"
            ],
            "initializationOptions": {}
        },
    },
    "Lua": {
        "runtime": {
            "version": "LuaJIT",
            "path": [
                "?.lua",
                "?/init.lua",
                "lua/?.lua"
            ]
        },
        "diagnostics": {
            "globals": [
                "vim",
                "hs"
            ]
        },
        "workspace": {
            "library": [
                "/usr/share/nvim/runtime/lua"
            ],
            "maxPreload": 1000,
            "preloadFileSize": 1000
        },
        "format": {
            "enable": true
        },
        "useSandbox": false
    },
    "python": {
        "pythonPath": "${userHome}/.pyenv/versions/3.12.0/bin/python",
        "linting": {
            "enabled": true,
            "flake8Enabled": true,
            "mypyEnabled": true
        },
        "formatting": {
            "provider": "autopep8"
        }
    },
    "json.schemas": [
        {
            "fileMatch": [
                "package.json"
            ],
            "url": "https://json.schemastore.org/package.json"
        }
    ],
    "suggest.autoTrigger": "always",
    "suggest.noselect": false,
    "suggest.minTriggerInputLength": 2,
    "suggest.timeout": 300,
    "diagnostic.enable": true,
    "coc.preferences.formatOnSaveFiletypes": [
        "python",
        "sh",
        "lua",
        "json"
    ],
    "signature.enable": true,
    "hover.enable": true,
    "inlayHint.enable": false,
    "suggest.preview": true,
    "snippets.expand": "UltiSnips#ExpandSnippet()",
    "coc.preferences.menu.bgColor": "#1a1b26",
    "coc.preferences.menu.border": "rounded",
    "coc.preferences.menu.highlightSelected": true,
    "coc.preferences.menu.default": "tokyonight",
    "coc.preferences.colorscheme": "tokyonight",
}
